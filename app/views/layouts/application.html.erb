<!DOCTYPE html>
<html>
<head>
  <title><%= (@title.present?) ? (@title) : 'Instagram'%></title>
  <%= stylesheet_link_tag    'bootstrap.min', media: 'all', 'data-turbolinks-track' => true %>
  <%= stylesheet_link_tag    'default', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'bootstrap.min', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'jquery-2.1.4.min', 'data-turbolinks-track' => true %>
  <%= favicon_link_tag 'instagram.png' %>
  <%= csrf_meta_tags %>
</head>
<body>
  <div class="container">
    <%= yield %>
  </div>
</body>
</html>
